{
  "manifest_version": 3,
  "name": "Tab for a Cause",
  "description": "Raise money for charity with every browser tab you open!",
  "icons": {
    "16": "./img/logo16.png",
    "32": "./img/logo32.png",
    "48": "./img/logo48.png",
    "128": "./img/logo128.png"
  },
  "background": {
    "service_worker": "ext-background.js"
  },
  "commands": {
    "open-tab": {
      "suggested_key": {
        "default": "Ctrl+K"
      },
      "description": "Open Tab for a Cause."
    }
  },
  "permissions": ["tabs"],
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
